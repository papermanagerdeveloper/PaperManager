<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">


    <servlet>
        <servlet-name>AuthenticationServlet</servlet-name>
        <servlet-class>AuthenticationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AuthenticationServlet</servlet-name>
        <url-pattern>/AuthenticationServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UploadHandleServlet</servlet-name>
        <servlet-class>UploadHandleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadHandleServlet</servlet-name>
        <url-pattern>/UploadHandleServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DownloadHandleServlet</servlet-name>
        <servlet-class>DownloadHandleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DownloadHandleServlet</servlet-name>
        <url-pattern>/DownloadHandleServlet</url-pattern>
    </servlet-mapping>

</web-app>